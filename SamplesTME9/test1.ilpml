class Point extends Object
{
    var x;

    method do()(
        self.x
    )

}


class PointLess extends Point {
    var y ; 
    method do()(
        super + y
    )
}


let t = new PointLess(0, 2) and i = 0 in (
    while i < 100000 do (
        try (
            t.do();
            i = i + 1;
        ) catch (e) (
            false
        )
    );
    t.do();
)


